<div class="content-fiuld room">
  <h4 class="pb-4">検索結果:<%= @rooms.count %>件</h4>
  <div class="row">
    <div class="col-3">
      <div class="room-frame-inner border mb-5">
        <% @rooms.each do |room| %>
        <div class="room-image-out">
          <%= image_tag room.room_image.to_s,class: "room-image-search" %>
        </div>
          <%= link_to "#{ room.room_name }", room %>
          <%= room.room_adress %>
          <%= room.room_price %>
        <% end %>
      </div>
    </div>
  </div>
</div>
